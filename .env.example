# PostgreSQL connection string
# Replace username, password, and dbname with your local Postgres values.
DATABASE_URL=postgres://username:password@localhost:5432/crowdspace

# Server port (optional, defaults to 3000)
PORT=3000

# Session secret for admin authentication (change this to a random string)
SESSION_SECRET=your-secret-key

# Google embedding API for AI-powered search (optional)
# Get an API key from: https://aistudio.google.com/
GOOGLE_API_KEY=your-google-api-key

# Embedding model name (optional, defaults to text-embedding-004)
GEMMA_EMBEDDING_MODEL=text-embedding-004
